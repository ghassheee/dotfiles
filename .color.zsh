alias blue='echo -en "\033[36m"'
alias red='echo -en "\033[31m"'
alias yellow='echo -en "\033[33m"'
alias green='echo -en "\033[32m"'
alias purple='echo -en "\033[34m"'
alias orange='echo -en "\033[35m"'
alias white='echo -en "\033[0m"'

alias formatOutput='while read line; do blue; echo $line; red; done'


